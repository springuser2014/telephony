<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='start'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>

    <inherits name="com.google.gwt.uibinder.UiBinder"/>

    <inherits name="com.google.gwt.http.HTTP"/>

    <!-- We need the JUnit module in the main module,               -->
    <!-- otherwise eclipse complains (Google plugin bug?)           -->
    <inherits name='com.google.gwt.junit.JUnit'/>

    <!-- For production, most teams prefer to set the default log level to `OFF` -->
    <!--<inherits name="com.allen_sauer.gwt.log.gwt-log-OFF" />-->

    <!-- For development, a default of `DEBUG` is recommended -->
    <inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG"/>
    <!--<set-property name="log_WindowLogger" value="DISABLED" />-->
    <!--<set-property name="log_FirebugLogger" value="ENABLED" />-->
    <set-property name="log_DivLogger" value="ENABLED"/>

    <!-- Some SmartGWT modules config -->
    <inherits name="com.smartgwt.SmartGwtNoTheme"/>
    <inherits name="com.smartclient.theme.enterpriseblue.EnterpriseBlue"/>
    <inherits name="com.smartclient.theme.enterpriseblue.EnterpriseBlueResources"/>

    <!-- Gilead/GWT/JPA integration -->
    <inherits name='net.sf.gilead.Gilead4Gwt'/>

    <!-- GWT INjection - guice for GWT -->
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.gwtplatform.mvp.Mvp"/>
    <inherits name="com.gwtplatform.dispatch.Dispatch"/>

    <!-- Google Guava for GWT -->
    <inherits name="com.google.common.collect.Collect"/>
      
    <inherits name="com.google.common.base.Base"/>

    <inherits name='org.restlet.Restlet'/>

    <inherits name="telephony.core"/>

    <define-configuration-property name="gin.ginjector" is-multi-valued="false"/>

    <set-configuration-property name="gin.ginjector" value="telephony.gwt.client.gin.GuiInjector"/>

    <!-- Google Guava for GWT -->
    <!--<inherits name="com.google.common.collect.Collect"/>-->

    <!--Tempraily compilation for FF -->
    <!--<set-property name="user.agent" value="gecko1_8"/>-->
    <!--Tempraily compilation for Chrome -->
    <set-property name="user.agent" value="safari"/>

    <!-- Other module inherits                                      -->

    <!-- Specify the app entry point class.                         -->
    <entry-point class='telephony.gwt.client.gwtp.StartPoint'/>

    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>
    <source path=''/>

</module>
